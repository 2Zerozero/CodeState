{
  "extends": "eslint:recommended",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true,
    "jest": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "no-unused-vars": "off"
  },
  "globals": {
    "delay": "readonly",
    "sleep": "readonly"
  }
}